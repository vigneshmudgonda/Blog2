{
  "name": "blog-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "quill-drag-and-drop-module": "^0.3.0",
    "quill-image-resize-module-react": "^3.0.0",
    "quill-table-ui": "^1.0.7",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.24.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node sitemap-generator.js && react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
"reactSnap": {
  "crawl": false,
  "include": [
    "/", "/about", "/contact", "/reviews",
    "/privacy-policy", "/terms", "/disclaimer"
  ],
  "skip": ["/login", "/signup", "/dashboard", "/blog", "/admin"],
  "inlineCss": true,
  "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
  "timeout": 60000,
  "source": "build",
  "destination": "build"
}
,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-snap": "^1.23.0"
  }
}
